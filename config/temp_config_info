    train_data_file = 'datasets/2fdiff_eq_p1_e3_smallerdomain_train.csv'
    test_data_file = 'datasets/2fdiff_eq_p1_e3_smallerdomain_test.csv'
    combo_data_file = 'datasets/2fdiff_eq_p1_e3_smallerdomain_train.csv'
    p = 4
    num_nodes_labeling = 40
    num_labels = 2
    n_epochs = 500
    example_type = 'Parabolic'

